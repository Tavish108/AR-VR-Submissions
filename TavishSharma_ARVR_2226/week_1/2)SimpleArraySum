import java.util.Scanner;
public class array {
public static void main(String[] args) {
            
            Scanner sc = new Scanner(System.in);
            int size = sc.nextInt();
            int[] a = new int[size];
            for(int i =0;i < size; i++)
            {
                a[i] = sc.nextInt();
            }
            int s = 0;
            for(int i=0; i < size; i++)
            {
                s = s + a[i];
            }
     System.out.print(s);
        }
    
    }
