class Solution{
    static long factorial(int N){
        int fact=1;
         for(int i=1;i<=N;i++)
        {
            fact = fact*i;
        }
        return fact;
    }
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        long result = factorial(n);
        System.out.println(result);
    }
}
