import java.util.Scanner;
public class OOPS {
public static void main(String[] args) {


int n, c = 0,arm = 0,a,b;
Scanner sc = new Scanner(System.in);
     n = sc.nextInt();
    b=n;
   while(n!=0)
   {
    n = n/10;
    c++;
   }
   n=b;
   while(n!=0)
   {
    a = n % 10;
    arm = (int) Math.pow(a,c)+ arm;
     n = n/10;
   }
   if(arm == b)
   {
    System.out.println("yes");
   }
   else
   {
    
    System.out.println("no");
   }

   sc.close();

}
}
